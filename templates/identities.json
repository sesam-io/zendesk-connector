[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-{{@ parent @}}-collect",
      "type": "dataset"
    },
    "pump": {
      "rescan_run_count": 1
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "properties",
              [
                "dict",
                "{{@ parent_id @}}",
                "_S.id"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "{{@ datatype @}}-list",
        "side_effects": false,
        "system": "{{@ system @}}",
        "trace": {
          "log_request_body_maxsize": 100000,
          "log_response_body_maxsize": 100000
        },
        "type": "rest"
      },
      {
        "rules": {
          "add-ids": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "_id",
              [
                "string",
                "_S.id"
              ]
            ]
          ],
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "create",
              [
                "apply",
                "add-ids",
                "_S.response"
              ]
            ],
            [
              "discard"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "batch_size": 1,
    "namespaced_identifiers": false,
    "sink": {
      "deletion_tracking": false,
      "set_initial_offset": "onload"
    },
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-transform",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create-payload": [
            [
              "copy",
              "*",
              [
                "list",
                "_*",
                "$*",
                "{{@ parent_id @}}"
              ]
            ]
          ],
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "$payload",
              [
                "dict",
                "{{@ payload_property @}}",
                [
                  "apply",
                  "create-payload",
                  "_S."
                ]
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "operation_delete": "{{@ datatype @}}-delete",
          "operation_insert": "{{@ datatype @}}-insert",
          "operation_lookup": "{{@ datatype @}}-lookup",
          "operation_update": "{{@ datatype @}}-update",
          "primary_key": "id",
          "rest_system": "{{@ system @}}",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-share-rest",
        "trace": {
          "log_request_body_maxsize": 100000,
          "log_response_body_maxsize": 100000
        },
        "type": "template"
      }
    ],
    "type": "pipe"
  }
]
{
  "_id": "{{@ system @}}-{{@ datatype @}}-collect",
  "namespaced_identifiers": false,
  "exclude_completeness": [
    "{{@ system @}}-{{@ datatype @}}-share"
  ],
  "source": {
    "dataset": "{{@ system @}}-{{@ parent @}}-collect",
    "type": "dataset"
  },
  "transform": [
    {
      "rules": {
        "default": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "properties",
            [
              "dict",
              "{{@ parent_id @}}",
              "_S.id"
            ]
          ]
        ]
      },
      "type": "dtl"
    },
    {
      "operation": "{{@ datatype @}}-list",
      "side_effects": false,
      "system": "{{@ system @}}",
      "trace": {
        "log_request_body_maxsize": 100000,
        "log_response_body_maxsize": 100000
      },
      "type": "rest"
    },
    {
      "rules": {
        "add-ids": [
          [
            "copy",
            "*"
          ],
          [
            "add",
            "_id",
            [
              "string",
              "_S.id"
            ]
          ],
          [
            "add",
            "$last-modified",
            [
              "datetime-parse",
              "%Y-%m-%dT%H:%M:%SZ",
              [
                "coalesce",
                [
                  "list",
                  "_S.updated_at",
                  "_S.created_at"
                ]
              ]
            ]
          ]
        ],
        "default": [
          [
            "copy",
            "*"
          ],
          [
            "create",
            [
              "apply",
              "add-ids",
              "_S.response"
            ]
          ],
          [
            "discard"
          ]
        ]
      },
      "type": "dtl"
    }
  ],
  "type": "pipe"
}
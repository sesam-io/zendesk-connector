{
  "additional_parameters": {
    "email": {},
    "subdomain": {}
  },
  "auth": "api_key",
  "datatypes": {
    "identities": {
      "parameters": {
        "parent_id": "user_id",
        "payload_property": "identity"
      },
      "parent": "users",
      "template": "templates/identities.json"
    },
    "organizations": {
      "parameters": {
        "payload_property": "organization"
      },
      "template": "templates/tickets.json"
    },
    "ticketcomments": {
      "parameters": {
        "parent_id": "ticket_id"
      },
      "parent": "tickets",
      "template": "templates/ticketcomments.json"
    },
    "tickets": {
      "parameters": {
        "payload_property": "ticket"
      },
      "template": "templates/tickets.json"
    },
    "users": {
      "parameters": {
        "payload_property": "user"
      },
      "sync_frequency": "slow",
      "template": "templates/tickets.json"
    }
  },
  "system": "Zendesk",
  "system-template": "templates/system.json"
}

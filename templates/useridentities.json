{
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-users-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "properties",
              [
                "dict",
                "user_id",
                "_S.id"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "operation": "{{@ datatype @}}-list",
        "side_effects": false,
        "system": "{{@ system @}}",
        "trace": {
          "log_request_body_maxsize": 100000,
          "log_response_body_maxsize": 100000
        },
        "type": "rest"
      },
      {
        "rules": {
          "add-ids": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "_id",
              ["string", "_S.id"]
            ]
          ],
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "create",
              [
                "apply",
                "add-ids",
                "_S.response.foo"
              ]
            ],
            [
              "discard"
            ]
          ]
        },
        "type": "dtl"
      }
    ],
    "type": "pipe"
  }